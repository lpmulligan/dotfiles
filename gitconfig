[user]
[core]
	editor = /usr/bin/vim
	excludesfile = /Users/lpm/.gitignore_global
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[color]
	ui = auto
[alias]
	s = status -s
	lg = log --oneline --all --graph --decorate
	br = branch
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
	ci = commit
	co = checkout
[include]
        path = ~/.gitconfig_local
